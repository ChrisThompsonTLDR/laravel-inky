{
    "name": "christhompsontldr/laravel-inky",
    "description": "Foundation Inky email templates in Laravel",
    "keywords": ["foundation", "inky", "laravel", "email", "mail"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Thompson",
            "email": "christhompsontldr@gmail.com"
        }
    ],
    "require": {
        "filipegar/inky": "^1.3.7",
        "illuminate/support": "~5.1|~6.0|~7.0",
        "illuminate/view": "~5.1|~6.0|~7.0",
        "symfony/css-selector": "^2.7|^3.0|^4.0",
        "symfony/dom-crawler": "^2.7|^3.0|^4.0",
        "tijsverkoyen/css-to-inline-styles": "^2.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.4",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "Christhompsontldr\\LaravelInky\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Christhompsontldr\\Tests\\LaravelInky\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Christhompsontldr\\LaravelInky\\InkyServiceProvider"
            ]
        }
    }
}
